   "source": [
    "# Finding k movies with the most similar user engagement\n",
    "from sklearn.neighbors import NearestNeighbors\n",
    "\n",
    "def find_similar_movies(movie_id, sparse, movie_mapper, movie_inv_mapper, k, metric='cosine'):\n",
    "    \"\"\"\n",
    "    Finds k-nearest neighbours for a given movie id.\n",
    "    \n",
    "    Args:\n",
    "        movie_id: Id of the movie of interest\n",
    "        sparse: User-item utility matrix\n",
    "        movie_mapper: Dictionary mapping movie ids to movie indices\n",
    "        movie_inv_mapper: Dictionary mapping movie indices to movie ids\n",
    "        k: Number of similar movies to retrieve\n",
    "        metric: Distance metric for kNN calculations\n",
    "    \n",
    "    Returns:\n",
    "        List of k similar movie IDs\n",
    "    \"\"\"\n",
    "    # Transpose the utility matrix for item-based kNN\n",
    "    sparse = sparse.T\n",
    "    neighbour_ids = []\n",
    "    \n",
    "    # Get the index of the movie of interest\n",
    "    movie_ind = movie_mapper[movie_id]\n",
    "    movie_vec = sparse[movie_ind]\n",
    "    \n",
    "    # Reshape the movie vector if needed\n",
    "    if isinstance(movie_vec, (np.ndarray)):\n",
    "        movie_vec = movie_vec.reshape(1, -1)\n",
    "    \n",
    "    # Use k+1 since kNN output includes the movieId of interest\n",
    "    kNN = NearestNeighbors(n_neighbors=k+1, algorithm=\"brute\", metric=metric)\n",
    "    kNN.fit(sparse)\n",
    "    \n",
    "    # Find k-nearest neighbors\n",
    "    neighbour = kNN.kneighbors(movie_vec, return_distance=False)\n",
    "    \n",
    "    # Extract movie ids from the indices\n",
    "    for i in range(0, k):\n",
    "        n = neighbour.item(i)\n",
    "        neighbour_ids.append(movie_inv_mapper[n])\n",
    "    \n",
    "    # Remove the movieId of interest from the list\n",
    "    neighbour_ids.pop(0)\n",
    "    \n",
    "    return neighbour_ids"
   ]
