   "source": [
    "# Count the number of unique movies in the movies dataset\n",
    "num_unique_movies = movies['movieId'].nunique()\n",
    "\n",
    "# Print the result\n",
    "print(f\"There are {num_unique_movies} unique movies in our movies dataset.\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 126,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Extract unique genres from the 'genres' column in the movies DataFrame\n",
    "genres = set(g for genres in movies['genres'] for g in genres)\n",
    "\n",
    "# Create binary columns for each genre indicating whether the movie belongs to that genre\n",
    "for g in genres:\n",
    "    movies[g] = movies.genres.transform(lambda x: int(g in x))\n",
    "\n",
    "# Create a new DataFrame with binary genre columns, excluding 'movieId', 'title', and 'genres'\n",
    "movie_genres = movies.drop(columns=['movieId', 'title', 'genres'])"
