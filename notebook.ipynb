   "source": [
    "# Calculate the average rating for movies\n",
    "m = movie_stats['mean'].mean()\n",
    "\n",
    "# Calculate the average number of ratings for movies\n",
    "C = movie_stats['count'].mean()\n",
    "\n",
    "print(f\"Avg number of ratings for movie i: {C}\")\n",
    "print(f\"Avg rating for a movie i: {m}\")"
   ]
