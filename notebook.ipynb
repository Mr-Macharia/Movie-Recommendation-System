  {
   "cell_type": "code",
   "execution_count": 109,
   "metadata": {},
   "outputs": [],
   "source": [
    "#define bayesian avg ft\n",
    "def bayesian_avg(ratings):\n",
    "    bayesian_avg = (C*m+ratings.sum())/ (C+ratings.count())\n",
    "    return round(bayesian_avg, 3)"
   ]
